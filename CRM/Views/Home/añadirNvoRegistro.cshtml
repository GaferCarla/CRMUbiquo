
@{
    ViewBag.Title = "añadirNvoRegistro";
    @Styles.Render("~/Content/registroStyle.css");
    @Styles.Render("~/Content/webPixel.css");

    @Styles.Render("~/Content/breadcrumb.css");
}

<div class="container py-4 card shadow border-0 mb-7" id="dataRegister">

    <div class="template-demo">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb breadcrumb-custom ">
                <li class="breadcrumb-item"><a href='@Url.Action("Index","Home")' data-abc="true" class="font-weight-bold text-uppercase">Home</a></li>
                <li class="breadcrumb-item"><a href='@Url.Action("Prospectos","Home")' data-abc="true" class="font-weight-bold text-uppercase">&nbsp&nbspProspectos</a></li>
                <li class="breadcrumb-item active font-weight-bold " aria-current="page"><span>&nbsp&nbspNuevo</span></li>
            </ol>
        </nav>

    </div>



    <div class=" flex-grow-1 overflow-visible">

        <main class="py-6 ">
            <div class="container-fluid">
                <!-- Card stats -->


                <div class="wrapper bg-white">
                    @Html.Partial("_datosForm")

                    <br />
                    <hr />
                    <br />
                    @Html.Partial("_registroProspeccion")
                    <div class="py-3 pb-4 border-bottom">
                        <a class="btn btn-primary mr-3" style="color:white; font-weight:700">Guardar</a>

                        <a href="@Url.Action("Prospectos","Home")" class="btn border btn-secondary" style="font-weight:700">Cancelar</a>
                    </div>

                </div>

            </div>
        </main>
    </div>

</div>












